<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LibraryController</servlet-name>
  	<display-name>LibraryController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.LibraryController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.LoginController</servlet-class>
  </servlet>
  
  <servlet>
		<servlet-name>StudentController</servlet-name>
		<display-name>StudentController</display-name>
		<description></description>
		<servlet-class>com.jac.web.controller.StudentController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditStudentController</servlet-name>
		<display-name>EditStudentController</display-name>
		<description></description>
		<servlet-class>com.jac.web.controller.EditStudentController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteStudentController</servlet-name>
		<display-name>DeleteStudentController</display-name>
		<description></description>
		<servlet-class>com.jac.web.controller.DeleteStudentController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentController</servlet-name>
		<url-pattern>/Student</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditStudentController</servlet-name>
		<url-pattern>/EditStudent</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteStudentController</servlet-name>
		<url-pattern>/DeleteStudent</url-pattern>
	</servlet-mapping>
	
  <servlet-mapping>
  	<servlet-name>LibraryController</servlet-name>
  	<url-pattern>/Library</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
</web-app>
